.container{
    width: 100%;
    height: 360rpx;
    display: flex;
    flex-direction: row;
    align-content: center;
    justify-content: center;
    background-color: #fff;
}

.inner-container{
    width: 730rpx;
    height: 300rpx;
    border: 1px dashed #ccc;
    margin-top: 10rpx;
    margin-left: 10rpx;
}

.grid-item{
    width: 200rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.image{
    width: 60rpx;
    height: 60rpx;
}

.text {
    margin-top: 6rpx;
    font-size: 24rpx;
    color: #333;
}

